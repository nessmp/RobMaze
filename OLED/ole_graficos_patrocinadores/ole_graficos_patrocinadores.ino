  Serial.begin(9600);
}

void draw()
{
  if (frame == 0)
    u8g.drawBitmapP( 0, 0, 16, 64, John_Deere);
  else if (frame == 1)
    u8g.drawBitmapP( 0, 0, 16, 64, Yasaki);
  else if (frame == 2)
    u8g.drawBitmapP( 0, 0, 16, 64, Mexico);
  else if (frame == 3)
    u8g.drawBitmapP( 0, 0, 16, 64, KitMedico);
}

void loop() {
  u8g.firstPage();
  do {
    draw();
  } while (u8g.nextPage());

  frame ++;
  if (frame > 3)
    frame = 0;
  delay(1000);
}

